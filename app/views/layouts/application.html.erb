<!DOCTYPE html>
<html>
<head>
  <title>Partying</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
	<nav>
		<ul>
			
			<li><%= link_to "Login", login_path %></li>
			<li><%= link_to "create an event", events_path %></li>
			<li><%= link_to "Logging Off", logout_path %></li>
			<li><%= link_to "Join The Party", new_user_path %></li>
		</ul>
		</nav>
<% if flash[:notice]%>
<p><%=flash[:notice]%></p>
<%end%>

<% if flash[:alert]%>
<p><%=flash[:alert]%></p>
<%end%>

<%= yield %>

</body>
</html>
